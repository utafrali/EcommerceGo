{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Connection Pool — Active / Idle / Total",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "stacking": { "mode": "none" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "last"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "db_pool_acquired_connections{service=~\"$service\"}",
          "legendFormat": "{{service}} — acquired",
          "refId": "A"
        },
        {
          "expr": "db_pool_idle_connections{service=~\"$service\"}",
          "legendFormat": "{{service}} — idle",
          "refId": "B"
        },
        {
          "expr": "db_pool_total_connections{service=~\"$service\"}",
          "legendFormat": "{{service}} — total",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Max Connections vs Total",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*max.*" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "last"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "db_pool_total_connections{service=~\"$service\"}",
          "legendFormat": "{{service}} — total",
          "refId": "A"
        },
        {
          "expr": "db_pool_max_connections{service=~\"$service\"}",
          "legendFormat": "{{service}} — max",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Pool Utilization %",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "red", "value": 0.9 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "db_pool_acquired_connections{service=~\"$service\"} / db_pool_max_connections{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Connection Acquire Rate (per second)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(db_pool_acquire_count_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Avg Acquire Latency (per acquire)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(db_pool_acquire_duration_seconds_total{service=~\"$service\"}[5m]) / rate(db_pool_acquire_count_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Empty Acquire Rate (waited for conn)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 30
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(db_pool_empty_acquire_count_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Canceled Acquire Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 30
          },
          "color": { "mode": "fixed", "fixedColor": "red" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(db_pool_canceled_acquire_count_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Connection Lifecycle — New / Destroyed",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 30,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(db_pool_new_connections_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}} — new",
          "refId": "A"
        },
        {
          "expr": "rate(db_pool_max_lifetime_destroy_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}} — lifetime destroy",
          "refId": "B"
        },
        {
          "expr": "rate(db_pool_max_idle_destroy_total{service=~\"$service\"}[5m])",
          "legendFormat": "{{service}} — idle destroy",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Constructing Connections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "db_pool_constructing_connections{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Connection Pool Summary",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "" },
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Utilization" },
            "properties": [
              { "id": "unit", "value": "percentunit" },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 0.7 },
                    { "color": "red", "value": 0.9 }
                  ]
                }
              },
              { "id": "custom.displayMode", "value": "color-background" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Utilization", "desc": true }]
      },
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "service" } }
      ],
      "targets": [
        {
          "expr": "db_pool_acquired_connections",
          "legendFormat": "{{service}}",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "db_pool_idle_connections",
          "legendFormat": "{{service}}",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "db_pool_total_connections",
          "legendFormat": "{{service}}",
          "refId": "C",
          "format": "table",
          "instant": true
        },
        {
          "expr": "db_pool_max_connections",
          "legendFormat": "{{service}}",
          "refId": "D",
          "format": "table",
          "instant": true
        },
        {
          "expr": "db_pool_acquired_connections / db_pool_max_connections",
          "legendFormat": "{{service}}",
          "refId": "E",
          "format": "table",
          "instant": true
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["ecommerce", "database", "pool", "postgresql"],
  "templating": {
    "list": [
      {
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "" },
        "definition": "label_values(db_pool_total_connections, service)",
        "includeAll": true,
        "multi": true,
        "name": "service",
        "query": "label_values(db_pool_total_connections, service)",
        "refresh": 2,
        "regex": "",
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Database Connection Pool",
  "uid": "ecommerce-db-pool",
  "version": 1
}
